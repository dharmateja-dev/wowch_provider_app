# Stripe API Keys
# Copy this file to .env and replace with your actual keys
# NEVER commit your actual .env file to version control

# Stripe Secret Key (used for server-side API calls)
# For testing: sk_test_...
# For production: sk_live_...
STRIPE_SECRET_KEY=your_stripe_secret_key_here

# Stripe Publishable Key (used for client-side SDK)
# For testing: pk_test_...
# For production: pk_live_...
STRIPE_PUBLISHABLE_KEY=your_stripe_publishable_key_here

# Stripe API URL (usually doesn't need to be changed)
STRIPE_API_URL=https://api.stripe.com/v1/payment_intents
